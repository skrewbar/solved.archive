   "source": [
    "# [OX퀴즈](https://www.acmicpc.net/problem/8958)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "for t in range(int(input())):\n",
    "    score = 0\n",
    "    streak = 0\n",
    "    result = input()\n",
    "    for char in result:\n",
    "        if char == \"O\":\n",
    "            score += 1 + streak\n",
    "            streak += 1\n",
    "        else:\n",
    "            streak = 0\n",
    "    print(score)"
   ]
